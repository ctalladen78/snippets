{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "*",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "aws:RequestedRegion": [
            "ap-southeast-1"
          ]
        },
        "Bool": {
          "aws:MultiFactorAuthPresent": "true"
        },
        "Null": {
          "aws:MultiFactorAuthAge": "false"
        }
      }
    }
  ]
}
